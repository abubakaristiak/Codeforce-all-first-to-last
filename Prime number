#include <stdio.h>

int main()
{
    int n;
    scanf("%d", &n);
    int temp = 0;
    for(int i== 2, i<n, i++){
        if(n%i == 0){
           temp=1;
        }
    }
    
    if(temp==0){
        printf("Yes");
    }
    else{
        printf("No");
    }
    

    return 0;
}
